{
  "name": "previous-admin",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "npm run generate:prod && npm run build",
    "build:backend": "npx -y tsc -p backend/tsconfig.json",
    "build:backend:bundle": "esbuild backend/index.ts --bundle --platform=node --target=node20 --outfile=backend/dist/bundle.js --sourcemap",
    "build:backend:local": "bash scripts/build/backend.sh",
    "smoke-test": "bash scripts/build/smoke-test.sh",
    "start:backend": "node backend/dist/bundle.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "backend": "tsx --tsconfig backend/tsconfig.json backend/index.ts",
    "restart:backend": "pkill -f \"backend/index.ts\" || true && npm run backend &",
    "update": "sudo bash scripts/update/admin.sh",
    "start": "npm run preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "vitest run --run frontend",
    "test:backend": "vitest run --run backend",
    "generate": "npm run generate:schema && npm run generate:translationkeys && npm run generate:fontlist && npm run generate:screenshots",
    "generate:prod": "npm run generate:schema && npm run generate:translationkeys && npm run generate:fontlist",
    "generate:schema": "bash scripts/build/schema.sh",
    "generate:fontlist": "tsx scripts/build/fontlist.ts",
    "generate:screenshots": "node scripts/build/screenshots.mjs",
    "generate:translationkeys": "tsx scripts/build/translations.ts",
    "import:exampledb": "tsx scripts/dev/import-db.ts"
  },
  "dependencies": {
    "better-sqlite3": "^12.5.0",
    "cors": "^2.8.5",
    "express": "^5.2.1",
    "ini": "^6.0.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.5.1",
    "remark-gfm": "^4.0.1",
    "rsuite": "^6.1.0",
    "sf-symbols-lib": "github:LAYEREDwork/sf-symbols-lib",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/ini": "^4.1.1",
    "@types/node": "^25.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "esbuild": "^0.18.18",
    "eslint": "^9.9.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "express": "^5.2.1",
    "globals": "^17.0.0",
    "jsdom": "^27.3.0",
    "playwright": "^1.57.0",
    "postcss": "^8.4.35",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "glob": "^10.3.0"
  }
}
